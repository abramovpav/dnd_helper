{
  "name": "dndhelper",
  "description": "Dnd Helper project",
  "author": "Pavel Abramau <abramovpav@gmail.com>",
  "private": true,
  "scripts": {
    "browserify": "browserify -e src/main.jsx -o ../static/build/bundle.js",
    "watchify": "watchify -d -e src/main.jsx -o ../static/build/bundle.js -v",
    "buildCss": "node-sass css/bundle.scss ../static/build/bundle.css",
    "watchCss": "nodemon -e scss -x \"npm run buildCss\"",
    "dev": "npm-run-all --parallel watchify watchCss",
    "production": "npm-run-all browserify buildCss"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "js-cookie": "^2.1.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-data-grid": "^2.0.58",
    "react-data-grid-addons": "^2.0.58",
    "react-dnd": "^2.5.1",
    "react-dom": "^15.6.1",
    "react-jsonschema-form": "^0.50.1",
    "react-paginate": "^4.4.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-simple-dropdown": "latest",
    "react-sortable-tree": "^1.2.2",
    "react-tabs": "^2.0.0",
    "redux": "^3.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "watchify": "^2.6.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
