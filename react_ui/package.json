{
  "name": "dnd_helper",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Pavel Abramau",
    "email": "abramovpav@gmail.com"
  },
  "description": "Helper for party in DnD",
  "main": "main.js",
  "scripts": {
    "watch-js": "watchify -e src/main.js -v -t [ babelify ] -o ../static/build/bundle.js",
    "build-css": "node-sass --include-path scss scss/bundle.scss   ../static/build/bundle.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "npm-run-all --parallel watch-js watch-css"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react-cookies": "^0.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tabs": "^2.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "node-sass": "^4.7.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "watchify": "^3.9.0"
  }
}
